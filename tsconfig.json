{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "compilerOptions": {
      "baseUrl": ".",
      "outDir": "dist",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      }
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.d.ts",
      "src/**/*.tsx",
      "src/**/*.vue"
    ],
    "exclude": [
      "node_modules",
      "**/node_modules"
    ]
}